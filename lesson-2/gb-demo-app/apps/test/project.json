{
    "name": "test",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/test/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "@nrwl/webpack:webpack",
        "outputs": ["{options.outputPath}"],
        "defaultConfiguration": "production",
        "options": {
          "target": "node",
          "compiler": "tsc",
          "outputPath": "dist/apps/test",
          "main": "apps/test/src/main.ts",
          "tsConfig": "apps/test/tsconfig.app.json",
          "assets": ["apps/test/src/assets"],
          "isolatedConfig": true,
          "webpackConfig": "apps/test/webpack.config.js"
        },
        "configurations": {
          "development": {},
          "production": {}
        }
      },
      "serve": {
        "executor": "@nrwl/js:node",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "test:build"
        },
        "configurations": {
          "development": {
            "buildTarget": "test:build:development"
          },
          "production": {
            "buildTarget": "test:build:production"
          }
        }
      },
      "lint": {
        "executor": "@nrwl/linter:eslint",
        "outputs": ["{options.outputFile}"],
        "options": {
          "lintFilePatterns": ["apps/test/**/*.ts"]
        }
      },
      "test": {
        "executor": "@nrwl/jest:jest",
        "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
        "options": {
          "jestConfig": "apps/test/jest.config.ts",
          "passWithNoTests": true
        },
        "configurations": {
          "ci": {
            "ci": true,
            "codeCoverage": true
          }
        }
      }
    },
    "tags": []
  }